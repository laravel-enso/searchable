{
    "name": "laravel-enso/searchable",
    "description": "Searchable dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "searchable"
    ],
    "homepage": "https://github.com/laravel-enso/searchable",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Adrian Ocneanu",
        "email": "aocneanu@gmail.com",
        "homepage": "https://laravel-enso.com",
        "role": "Developer"
    }],
    "require": {
        "laravel-enso/core": "^6.0",
        "laravel-enso/filters": "^2.0",
        "laravel-enso/migrator": "^2.0",
        "laravel-enso/permissions": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Searchable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Searchable\\AppServiceProvider",
                "LaravelEnso\\Searchable\\SearchServiceProvider"
            ],
            "aliases": {
                "Search": "LaravelEnso\\Searchable\\Facades\\Search"
            }
        }
    }
}
